{
  "name": "6328-log-viewer",
  "productName": "6328 Log Viewer",
  "version": "1.0.0",
  "description": "Logging tool from FRC Team 6328.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron main.js",
    "build": "electron-builder build",
    "test-build": "electron-builder build --dir"
  },
  "author": "FRC 6328",
  "license": "MIT",
  "devDependencies": {
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "electron-window-state": "^5.0.3"
  },
  "build": {
    "appId": "org.littletonrobotics.logviewer",
    "publish": [],
    "directories": {
      "buildResources": "./"
    },
    "fileAssociations": [
      {
        "ext": "rlog",
        "name": "Robot log",
        "description": "Log file for an FRC robot.",
        "mimeType": "application/octet-stream",
        "role": "Viewer",
        "icon": "assets/rlog-icon"
      }
    ],
    "mac": {
      "target": "dmg",
      "icon": "assets/app-icon.icns"
    },
    "linux": {
      "target": [
        "deb",
        "rpm"
      ],
      "icon": "assets/app-icon.icns",
      "maintainer": "FRC 6328",
      "category": "Utility"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/app-icon.ico"
    }
  }
}